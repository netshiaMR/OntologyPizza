    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#SeefoodTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#SeefoodTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#ShisaNyamaHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#ShisaNyamaHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#NamedPizza"/>
