    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#SalamiTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#SalamiTopping">
