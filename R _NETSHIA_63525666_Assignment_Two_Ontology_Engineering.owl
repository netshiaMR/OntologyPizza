    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#RedPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#RedPepperTopping">
