<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#"
     xml:base="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’">
        <rdfs:comment>A tutorial ontology for the Pizza domain</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#hasSpiciness"/>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#isIngredientOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#isIngredientOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasIngredient"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasIngredient">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasSpiciness">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasIngredient"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasCaloricContent -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasCaloricContent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#Pizza"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#CaperTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#CheeseTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#DeepPanBase -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PeppreTopping"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#HamTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#JalapenoPepperTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#JalapenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PeppreTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#MeatTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#MeatTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#MozzarellaTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#MozzarellaTopping">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#CheeseTopping"/>
        <owl:disjointWith rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#ParmesanTopping"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#MushroomTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#MushroomTopping">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#OliveTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#OliveTopping">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#ParmesanTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#ParmesanTopping">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#CheeseTopping"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#ThinAndCrispyBase -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#ThinAndCrispyBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaBase"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#TomatoTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#TomatoTopping">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/pizza/pizza.owl#VegetableTopping -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#VegetableTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#AmericanaHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#AmericanaHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A pizza that only has
ShisaNyamaPizza  toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#AmericanaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A pizza that only has
Mozzarella and Tomato toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#AnchovyTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#SeefoodTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#CheesyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#InterestingPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#InterestingPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#MargheritaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#MargheritaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.co-ode.org/ontologies/pizza/pizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaTopping"/>
        <rdfs:comment xml:lang="en">A pizza that only has Mozzarella and Tomato toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#NamedPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#NamedPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#Pizza"/>
        <rdfs:comment xml:lang="en">A pizza that can be found on a pizza menu</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PepperoniTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PepperoniTopping">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PeppreTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PeppreTopping">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/pizza/pizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#Pizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#Pizza">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#hasBase"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaBase"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaBase -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaBase"/>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PizzaTopping"/>
    


    <!-- http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PrawnTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/netshiar2/ontologies/2021/5/PizzaTutorial’#PrawnTopping">
